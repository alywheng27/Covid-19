
    <!-- Contact -->
    <section id="contact">
      <div class="container">
        <div class="row">
          <!-- Get-in-touch --> 
          <div class="col-xl-4 col-lg-4 col-md-12 wow fadeInLeft" data-wow-delay="0.3s">
            <div class="contact-info">
              <h3>{{ content.field_video_title.0 }}</h3>
              <p>{{ content.field_video_desc.0 }}</p>
                <p class="address"><i class="fa fa-map-marker"></i><span>{{ content.field_testi_header.0 }} <span></p>

                <p class="address"><i class="fa fa-envelope"></i>
                  <span>
                    <a href="mailto:{{ content.field_video_url.0 }}">{{ content.field_video_url.0 }}</a>
                    <a href="mailto:{{ content.field_designation.0 }}">{{ content.field_designation.0 }}</a>
                  <span>
                </p>
                <p class="address"><i class="fa fa-phone"></i>
                  <span>
                    <a href="tel:{{ content.field_testi_name.0 }}">{{ content.field_testi_name.0 }}</a>
                    <a href="tel:{{ content.field_twitter_url.0 }}">{{ content.field_twitter_url.0 }}</a>
                  <span>
                </p>
              </div>
          </div>
          <!-- Get-in-touch end--> 

          <!-- Contact-form -->
           <div class="col-xl-8 col-lg-8 col-md-12 wow fadeInDown" data-wow-delay="0.6s">
            <div class="get-in-touch">
              <h3>{{ content.field_facebook_url.0 }}</h3>
              <p>{{ content.field_list_caption.0 }}</p>
                {{ content.field_contact_form.0 }}
            </div>
          </div> <!-- contact-form --> 

           <!-- Goolgle Map --> 
          <div class="col-xl-12 col-lg-12 col-md-12 wow fadeInUp" data-wow-delay="0.3s"> 
            <div id="map" class="animate-box" data-animate-effect="fadeIn">
             {{ content.field_map.0 }}
            </div>  
          </div> 


        </div>
      </div>
    </section>
    <!-- Contact End -->