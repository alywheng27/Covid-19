<nav>
<ul class="pagination text-center">
{% if items %}
      {# Print previous item if we are not on the first page. #}
      {% if items.previous %}
          <li><a href="{{ items.previous.href }}"  title="{{ 'Go to previous page'|t }}" rel="prev" {{ items.previous.attributes|without('href', 'title', 'rel') }}><i class="fa fa-chevron-left"></i> </a></li>
      {% endif %}
      {# Now generate the actual pager piece. #}
      {% for key, item in items.pages %}
          {% if current == key %}
               <li class="active"><a href="{{ item.href }}">{{- key -}}</a></li>
          {% else %}
              <li><a href="{{ item.href }}">{{- key -}}</a></li>
          {% endif %}
       {% endfor %}
      {# Print next item if we are not on the last page. #}
      {% if items.next %}
        <li><a href="{{ items.next.href }}" aria-label="Next" > <i class="fa fa-chevron-right"></i> </a></li>
      {% endif %}
     
{% endif %}
</ul>
</nav>

